5\\u094B \\u0905\\u092A\\u0928\\u0940 \\u0917\\u0932\\u0924\\u0940 \\u0915\\u093E \\u0905\\u0939\\u0938\\u093E\\u0938 \\u0939\\u0941\\u0906\\u0964 \\u0909\\u0938\\u0928\\u0947 \\u0938\\u092E\\u091D\\u093E \\u0915\\u093F \\u0938\\u091A\\u094D\\u091A\\u093E \\u091C\\u094D\\u091E\\u093E\\u0928 \\u092A\\u094D\\u0930\\u093E\\u092A\\u094D\\u0924 \\u0915\\u0930\\u0928\\u0947 \\u0915\\u0947 \\u0932\\u093F\\u090F, \\u0909\\u0938\\u0947 \\u092A\\u0939\\u0932\\u0947 \\u0935\\u093F\\u0928\\u092E\\u094D\\u0930 \\u0939\\u094B\\u0928\\u093E \\u0939\\u094B\\u0917\\u093E \\u0914\\u0930 \\u0905\\u092A\\u0928\\u093E \\u0915\\u091F\\u094B\\u0930\\u093E \\u0916\\u093E\\u0932\\u0940 \\u0915\\u0930\\u0928\\u093E \\u0939\\u094B\\u0917\\u093E\\u0964 \\u0909\\u0938\\u0928\\u0947 \\u0917\\u0941\\u0930\\u0941 \\u0915\\u0947 \\u0938\\u093E\\u092E\\u0928\\u0947 \\u091D\\u0941\\u0915\\u0915\\u0930 \\u0915\\u0939\\u093E, \\\"\\u092E\\u0948\\u0902 \\u0916\\u093E\\u0932\\u0940 \\u0915\\u091F\\u094B\\u0930\\u0947 \\u0915\\u0947 \\u0938\\u093E\\u0925 \\u0906\\u092A\\u0938\\u0947 \\u0938\\u0940\\u0916\\u0928\\u0947 \\u0915\\u0947 \\u0932\\u093F\\u090F \\u0924\\u0948\\u092F\\u093E\\u0930 \\u0939\\u0942\\u0902\\u0964\\\"\\n      \\n      \\u0917\\u0941\\u0930\\u0941 \\u092E\\u0941\\u0938\\u094D\\u0915\\u0941\\u0930\\u093E\\u090F \\u0914\\u0930 \\u0928\\u090F \\u0938\\u093F\\u0930\\u0947 \\u0938\\u0947 \\u091A\\u093E\\u092F \\u092C\\u0928\\u093E\\u0928\\u0947 \\u0932\\u0917\\u0947\\u0964 \\u0907\\u0938 \\u092C\\u093E\\u0930, \\u091C\\u092C \\u0935\\u0947 \\u091A\\u093E\\u092F \\u0921\\u093E\\u0932\\u0928\\u0947 \\u0932\\u0917\\u0947, \\u0924\\u094B \\u092F\\u0941\\u0935\\u0915 \\u0928\\u0947 \\u0927\\u094D\\u092F\\u093E\\u0928 \\u0938\\u0947 \\u0938\\u0941\\u0928\\u093E \\u0914\\u0930 \\u0938\\u0940\\u0916\\u093E, \\u0905\\u092A\\u0928\\u0947 \\u092E\\u0928 \\u0915\\u094B \\u0916\\u093E\\u0932\\u0940 \\u0915\\u0930\\u0915\\u0947 \\u0935\\u093E\\u0938\\u094D\\u0924\\u0935\\u093F\\u0915 \\u091C\\u094D\\u091E\\u093E\\u0928 \\u0915\\u0947 \\u0932\\u093F\\u090F \\u091C\\u0917\\u0939 \\u092C\\u0928\\u093E\\u0908\\u0964\\n      \\n      \\u0907\\u0938 \\u0915\\u0939\\u093E\\u0928\\u0940 \\u0938\\u0947 \\u0939\\u092E\\u0947\\u0902 \\u092F\\u0939 \\u0938\\u093F\\u0916 \\u092E\\u093F\\u0932\\u0924\\u0940 \\u0939\\u0948 \\u0915\\u093F \\u091C\\u094D\\u091E\\u093E\\u0928 \\u092A\\u094D\\u0930\\u093E\\u092A\\u094D\\u0924 \\u0915\\u0930\\u0928\\u0947 \\u0915\\u0947 \\u0932\\u093F\\u090F \\u0939\\u092E\\u0947\\u0902 \\u0905\\u092A\\u0928\\u0947 \\u0905\\u0939\\u0902\\u0915\\u093E\\u0930 \\u0914\\u0930 \\u092A\\u0942\\u0930\\u094D\\u0935\\u0927\\u093E\\u0930\\u0923\\u093E\\u0913\\u0902 \\u0915\\u094B \\u091B\\u094B\\u0921\\u093C\\u0928\\u093E \\u0939\\u094B\\u0917\\u093E\\u0964 \\u0915\\u0947\\u0935\\u0932 \\u090F\\u0915 \\u0916\\u093E\\u0932\\u0940 \\u0915\\u091F\\u094B\\u0930\\u093E \\u0939\\u0940 \\u0928\\u090F \\u0905\\u0928\\u0941\\u092D\\u0935\\u094B\\u0902 \\u0938\\u0947 \\u092D\\u0930\\u093E \\u091C\\u093E \\u0938\\u0915\\u0924\\u093E \\u0939\\u0948\\u0964\"}];// Navigate to individual story\nconst handleReadStory=story=>{setSelectedStory(story);setCurrentPage('storyDetail');};// Go back to stories list\nconst handleBackToList=()=>{setCurrentPage('storiesList');};// Get image for story\nconst getStoryImage=storyId=>{// Using placeholder images based on story ID\nreturn\"/api/placeholder/\".concat(400+storyId,\"/\").concat(300+storyId);};// Header component\n// Stories list component\nconst StoriesList=()=>/*#__PURE__*/_jsx(\"div\",{className:\"container mx-auto p-4\",children:/*#__PURE__*/_jsx(\"div\",{className:\"grid md:grid-cols-2 gap-6\",children:stories.map(story=>/*#__PURE__*/_jsxs(\"div\",{className:\"bg-white rounded-lg shadow-md hover:shadow-lg transition-shadow cursor-pointer overflow-hidden\",onClick:()=>handleReadStory(story),children:[/*#__PURE__*/_jsx(\"img\",{src:getStoryImage(story.id),alt:story.title,className:\"w-full h-48 object-cover\"}),/*#__PURE__*/_jsxs(\"div\",{className:\"p-6\",children:[/*#__PURE__*/_jsx(\"h2\",{className:\"text-xl font-bold text-gray-800 mb-2\",children:story.title}),/*#__PURE__*/_jsx(\"p\",{className:\"text-gray-600 mb-4\",children:story.summary}),/*#__PURE__*/_jsxs(\"button\",{className:\"text-blue-600 font-medium hover:text-blue-800 flex items-center\",onClick:()=>handleReadStory(story),children:[\"\\u092A\\u0942\\u0930\\u0940 \\u0915\\u0939\\u093E\\u0928\\u0940 \\u092A\\u0922\\u093C\\u0947\\u0902\",/*#__PURE__*/_jsx(\"svg\",{xmlns:\"http://www.w3.org/2000/svg\",className:\"h-5 w-5 ml-1\",viewBox:\"0 0 20 20\",fill:\"currentColor\",children:/*#__PURE__*/_jsx(\"path\",{fillRule:\"evenodd\",d:\"M10.293 5.293a1 1 0 011.414 0l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414-1.414L12.586 11H5a1 1 0 110-2h7.586l-2.293-2.293a1 1 0 010-1.414z\",clipRule:\"evenodd\"})})]})]})]},story.id))})});// Story detail component\nconst StoryDetail=()=>{if(!selectedStory)return null;return/*#__PURE__*/_jsx(\"div\",{className:\"container mx-auto p-4\",children:/*#__PURE__*/_jsxs(\"div\",{className:\"bg-white rounded-lg shadow-md overflow-hidden\",children:[/*#__PURE__*/_jsx(\"img\",{src:getStoryImage(selectedStory.id),alt:selectedStory.title,className:\"w-full h-64 object-cover\"}),/*#__PURE__*/_jsxs(\"div\",{className:\"p-6\",children:[/*#__PURE__*/_jsx(\"h1\",{className:\"text-2xl font-bold text-gray-800 mb-4 border-b pb-4\",children:selectedStory.title}),/*#__PURE__*/_jsx(\"div\",{className:\"prose max-w-none\",children:selectedStory.content.split('\\n\\n').map((paragraph,index)=>/*#__PURE__*/_jsx(\"p\",{className:\"mb-4 leading-relaxed text-gray-700\",children:paragraph.trim()},index))})]})]})});};// Render the appropriate page based on currentPage state\nreturn/*#__PURE__*/_jsxs(\"div\",{className:\"min-h-screen bg-gray-100\",children:[currentPage==='storiesList'&&/*#__PURE__*/_jsx(StoriesList,{}),currentPage==='storyDetail'&&/*#__PURE__*/_jsx(StoryDetail,{})]});};export default App;","map":{"version":3,"names":["React","useState","jsx","_jsx","jsxs","_jsxs","App","currentPage","setCurrentPage","selectedStory","setSelectedStory","stories","id","title","summary","content","handleReadStory","story","handleBackToList","getStoryImage","storyId","concat","StoriesList","className","children","map","onClick","src","alt","xmlns","viewBox","fill","fillRule","d","clipRule","StoryDetail","split","paragraph","index","trim"],"sources":["C:/Users/jaina/OneDrive/Desktop/RIP/rbl/src/components/StoryPage.js"],"sourcesContent":["import React, { useState } from 'react';\r\n\r\nconst App = () => {\r\n  const [currentPage, setCurrentPage] = useState('storiesList');\r\n  const [selectedStory, setSelectedStory] = useState(null);\r\n\r\n  // Sample stories data in Hindi with images\r\n  const stories = [\r\n    {\r\n      id: 1,\r\n      title: \"अहंकार का अंत\",\r\n      summary: \"एक राजा के अहंकार और उसके द्वारा सीखे गए सबक की कहानी\",\r\n      content: `एक बार की बात है, एक बहुत ही शक्तिशाली राजा था जो अपने ज्ञान और शक्ति के लिए प्रसिद्ध था। वह इतना अहंकारी हो गया था कि उसने घोषणा कर दी कि उसके राज्य में कोई भी उससे अधिक बुद्धिमान नहीं है।\r\n\r\n      एक दिन एक साधु उसके दरबार में आए। राजा ने उनसे कहा, \"मैं दुनिया का सबसे बुद्धिमान व्यक्ति हूँ। क्या आप मुझे कुछ सिखा सकते हैं जो मैं नहीं जानता?\"\r\n\r\n      साधु ने मुस्कुराते हुए एक छोटा सा पत्थर राजा को दिया और कहा, \"इस पत्थर को अपनी मुट्ठी में कसकर पकड़ें और सुबह तक ऐसे ही रखें। लेकिन याद रखें, न तो आप इसे गिरा सकते हैं और न ही किसी को दिखा सकते हैं।\"\r\n\r\n      राजा को यह आसान लगा। लेकिन जैसे-जैसे रात बीती, पत्थर उसके हाथ में भारी होता गया। उसका हाथ दर्द करने लगा, लेकिन अपने अहंकार के कारण वह पत्थर को गिराना नहीं चाहता था।\r\n\r\n      सुबह होते-होते, राजा अपना दर्द सहन नहीं कर पाया और उसने पत्थर को गिरा दिया। साधु, जो अभी भी वहां था, ने कहा, \"महाराज, आप जीवन में जितना अधिक ज्ञान और शक्ति प्राप्त करते हैं, उतना ही भारी बोझ आप उठाते हैं। अहंकार इस बोझ को और भी भारी बना देता है। कभी-कभी सबसे बड़ी बुद्धिमानी है यह स्वीकार करना कि हम सभी सीखने वाले हैं।\"\r\n\r\n      राजा ने साधु के चरणों में झुककर क्षमा माँगी और उस दिन से उसने अपने अहंकार को त्याग दिया। वह एक न्यायप्रिय और दयालु शासक बन गया, जो हमेशा सीखने के लिए तैयार रहता था।`\r\n    },\r\n    {\r\n      id: 2,\r\n      title: \"सच्चा धन\",\r\n      summary: \"एक व्यापारी की कहानी जिसने सच्चे धन का अर्थ समझा\",\r\n      content: `एक समृद्ध व्यापारी था जो अपनी संपत्ति बढ़ाने में लगा रहता था। उसके पास बहुत धन था, लेकिन वह हमेशा और अधिक चाहता था। उसका परिवार उसके धन के साथ रहता था, लेकिन उसके बिना अकेला महसूस करता था।\r\n\r\n      एक बार व्यापारी के घर एक संत आए। उन्होंने व्यापारी से पूछा, \"क्या आप सुखी हैं?\" व्यापारी ने उत्तर दिया, \"मैं धनी हूँ, इसलिए मैं सुखी हूँ।\"\r\n      \r\n      संत ने मुस्कुराते हुए कहा, \"मैं आपको सच्चा धन दिखाना चाहता हूँ।\" वे व्यापारी को एक गरीब परिवार के घर ले गए, जहाँ पूरा परिवार एक साथ बैठकर भोजन कर रहा था। उनके पास कम था, लेकिन वे हँस रहे थे, बातें कर रहे थे और एक-दूसरे का आनंद ले रहे थे।\r\n      \r\n      व्यापारी ने उनके चेहरों पर खुशी देखी और समझ गया कि सच्चा धन परिवार, प्रेम और एक साथ बिताया गया समय है। उस दिन से, व्यापारी ने अपने काम के समय को कम किया और परिवार के साथ अधिक समय बिताना शुरू किया।\r\n      \r\n      कुछ महीनों बाद, जब संत वापस आए, तो उन्होंने व्यापारी को सचमुच खुश पाया। व्यापारी ने कहा, \"आपने मुझे सच्चा धन दिखाया - मेरा परिवार और उनके साथ बिताया गया समय। अब मैं वास्तव में धनी और सुखी हूँ।\"`\r\n    },\r\n    {\r\n      id: 3,\r\n      title: \"तीन प्रश्न\",\r\n      summary: \"एक राजा की कहानी जिसने जीवन के तीन सबसे महत्वपूर्ण प्रश्नों का उत्तर खोजा\",\r\n      content: `एक राजा था जो जीवन के तीन सबसे महत्वपूर्ण प्रश्नों का उत्तर जानना चाहता था:\r\n      \r\n      सबसे महत्वपूर्ण समय कौन सा है?\r\n      सबसे महत्वपूर्ण व्यक्ति कौन है?\r\n      सबसे महत्वपूर्ण कार्य क्या है?\r\n      \r\n      राजा ने अपने राज्य के सभी विद्वानों से पूछा, लेकिन हर किसी का उत्तर अलग था। असंतुष्ट होकर, वह एक प्रसिद्ध ऋषि से मिलने जंगल में गया जो पहाड़ पर अकेले रहते थे।\r\n      \r\n      जब राजा पहुंचा, तो उसने देखा कि ऋषि मिट्टी खोद रहे थे। राजा ने अपने प्रश्न पूछे, लेकिन ऋषि उत्तर देने के बजाय अपना काम जारी रखे। राजा, जो युवा और मजबूत था, ने देखा कि ऋषि थक गए हैं, इसलिए उसने फावड़ा लिया और उनकी मदद करने लगा।\r\n      \r\n      काम खत्म करने के बाद, राजा ने फिर से अपने सवाल दोहराए। तभी, एक आदमी जंगल से भागता हुआ आया, खून से लथपथ था। राजा और ऋषि ने उसके घावों की देखभाल की और उसे आराम करने दिया।\r\n      \r\n      अगली सुबह, वह आदमी राजा से बोला, \"आपने मेरी जान बचाई। मैं आपका दुश्मन था, बदला लेने के लिए आपका पीछा कर रहा था। लेकिन आपकी दया ने मुझे बदल दिया है।\"\r\n      \r\n      जब वह आदमी चला गया, राजा ने ऋषि से कहा, \"अब मुझे मेरे सवालों के जवाब दीजिए।\"\r\n      \r\n      ऋषि ने मुस्कुराते हुए कहा, \"आपको जवाब मिल चुके हैं। सबसे महत्वपूर्ण समय 'अभी' है - यही एक समय है जब हमारे पास शक्ति होती है। सबसे महत्वपूर्ण व्यक्ति वह है जिसके साथ आप अभी हैं। और सबसे महत्वपूर्ण कार्य उस व्यक्ति के लिए अच्छा करना है - यही जीवन का उद्देश्य है।\"\r\n      \r\n      राजा ने समझ लिया कि जीवन का सच्चा ज्ञान किताबों या विद्वानों में नहीं, बल्कि वर्तमान क्षण में रहने और हर मिलने वाले के प्रति दयालु होने में है।`\r\n    },\r\n    {\r\n      id: 4,\r\n      title: \"खाली कटोरा\",\r\n      summary: \"एक शिष्य और गुरु की कहानी जो सीखने के लिए खाली होने के महत्व को दर्शाती है\",\r\n      content: `एक युवक किसी महान गुरु से ज्ञान प्राप्त करने के लिए उनके आश्रम पहुंचा। वह अपने ज्ञान पर बहुत गर्व करता था और विभिन्न शास्त्रों का अध्ययन कर चुका था।\r\n\r\n      जब वह गुरु से मिला, गुरु ने उसे चाय पीने के लिए आमंत्रित किया। जब गुरु चाय डाल रहे थे, तो वे युवक के कटोरे में चाय डालते ही जा रहे थे, भले ही वह ऊपर तक भर गया था और फर्श पर बहने लगी थी।\r\n      \r\n      युवक चिल्लाया, \"रुकिए! कटोरा पहले ही भर चुका है। और अधिक नहीं जा सकता!\"\r\n      \r\n      गुरु ने शांति से कहा, \"ठीक ऐसे ही, तुम भी अपने विचारों और पूर्वधारणाओं से भरे हुए हो। मैं तुम्हें कैसे ज्ञान दे सकता हूं जब तक तुम खाली होने के लिए तैयार नहीं हो?\"\r\n      \r\n      युवक को अपनी गलती का अहसास हुआ। उसने समझा कि सच्चा ज्ञान प्राप्त करने के लिए, उसे पहले विनम्र होना होगा और अपना कटोरा खाली करना होगा। उसने गुरु के सामने झुककर कहा, \"मैं खाली कटोरे के साथ आपसे सीखने के लिए तैयार हूं।\"\r\n      \r\n      गुरु मुस्कुराए और नए सिरे से चाय बनाने लगे। इस बार, जब वे चाय डालने लगे, तो युवक ने ध्यान से सुना और सीखा, अपने मन को खाली करके वास्तविक ज्ञान के लिए जगह बनाई।\r\n      \r\n      इस कहानी से हमें यह सिख मिलती है कि ज्ञान प्राप्त करने के लिए हमें अपने अहंकार और पूर्वधारणाओं को छोड़ना होगा। केवल एक खाली कटोरा ही नए अनुभवों से भरा जा सकता है।`\r\n    }\r\n  ];\r\n\r\n  // Navigate to individual story\r\n  const handleReadStory = (story) => {\r\n    setSelectedStory(story);\r\n    setCurrentPage('storyDetail');\r\n  };\r\n\r\n  // Go back to stories list\r\n  const handleBackToList = () => {\r\n    setCurrentPage('storiesList');\r\n  };\r\n\r\n  // Get image for story\r\n  const getStoryImage = (storyId) => {\r\n    // Using placeholder images based on story ID\r\n    return `/api/placeholder/${400 + storyId}/${300 + storyId}`;\r\n  };\r\n\r\n  // Header component\r\n  \r\n  \r\n\r\n  // Stories list component\r\n  const StoriesList = () => (\r\n    <div className=\"container mx-auto p-4\">\r\n     \r\n      <div className=\"grid md:grid-cols-2 gap-6\">\r\n        {stories.map(story => (\r\n          <div \r\n            key={story.id} \r\n            className=\"bg-white rounded-lg shadow-md hover:shadow-lg transition-shadow cursor-pointer overflow-hidden\"\r\n            onClick={() => handleReadStory(story)}\r\n          >\r\n            <img \r\n              src={getStoryImage(story.id)} \r\n              alt={story.title} \r\n              className=\"w-full h-48 object-cover\"\r\n            />\r\n            <div className=\"p-6\">\r\n              <h2 className=\"text-xl font-bold text-gray-800 mb-2\">{story.title}</h2>\r\n              <p className=\"text-gray-600 mb-4\">{story.summary}</p>\r\n              <button \r\n                className=\"text-blue-600 font-medium hover:text-blue-800 flex items-center\"\r\n                onClick={() => handleReadStory(story)}\r\n              >\r\n                पूरी कहानी पढ़ें\r\n                <svg xmlns=\"http://www.w3.org/2000/svg\" className=\"h-5 w-5 ml-1\" viewBox=\"0 0 20 20\" fill=\"currentColor\">\r\n                  <path fillRule=\"evenodd\" d=\"M10.293 5.293a1 1 0 011.414 0l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414-1.414L12.586 11H5a1 1 0 110-2h7.586l-2.293-2.293a1 1 0 010-1.414z\" clipRule=\"evenodd\" />\r\n                </svg>\r\n              </button>\r\n            </div>\r\n          </div>\r\n        ))}\r\n      </div>\r\n    </div>\r\n  );\r\n\r\n  // Story detail component\r\n  const StoryDetail = () => {\r\n    if (!selectedStory) return null;\r\n    \r\n    return (\r\n      <div className=\"container mx-auto p-4\">\r\n        <div className=\"bg-white rounded-lg shadow-md overflow-hidden\">\r\n          <img \r\n            src={getStoryImage(selectedStory.id)} \r\n            alt={selectedStory.title} \r\n            className=\"w-full h-64 object-cover\"\r\n          />\r\n          \r\n          <div className=\"p-6\">\r\n            <h1 className=\"text-2xl font-bold text-gray-800 mb-4 border-b pb-4\">{selectedStory.title}</h1>\r\n            <div className=\"prose max-w-none\">\r\n              {selectedStory.content.split('\\n\\n').map((paragraph, index) => (\r\n                <p key={index} className=\"mb-4 leading-relaxed text-gray-700\">{paragraph.trim()}</p>\r\n              ))}\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    );\r\n  };\r\n\r\n  // Render the appropriate page based on currentPage state\r\n  return (\r\n    <div className=\"min-h-screen bg-gray-100\">\r\n     \r\n      {currentPage === 'storiesList' && <StoriesList />}\r\n      {currentPage === 'storyDetail' && <StoryDetail />}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default App;"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,KAAQ,OAAO,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAA